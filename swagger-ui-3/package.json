{
  "name": "api-discovery-ui",
  "version": "0.1.0",
  "description": "API Discovery Swagger UI",
  "private": true,
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build-client": "webpack",
    "build-server": "babel -d dist/server server",
    "build": " npm run build-server && npm run build-client",
    "dist": "npm run clean && NODE_ENV=production npm run build -- -p",
    "start-server": "API_DISCOVERY_BASE_DIR=./dist/server babel-node server/index.js",
    "start": "webpack-dev-server --inline --content-base dist/client"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "swagger-ui": "3.0.7",
    "swagger-ui-dist": "3.0.7",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5",
    "yaml-js": "0.1.5"
  },
  "dependencies": {
    "koa": "2.2.0",
    "koa-router": "7.1.1",
    "koa-static": "3.0.0"
  }
}
